{
  "compilerOptions": {
    "baseUrl": "/",
    "target": "es2019",
    "module": "commonjs",
    "lib": [
      "es2019"
    ],
    "declaration": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "outDir": "build",
    "noEmitOnError": true,
    "resolveJsonModule": true,
    "stripInternal": true,
    "incremental": true,
    "skipLibCheck": true
  },
  "include": [
    "**/*.ts",
    "setup.mjs",
    "teardown.mjs"
, "src/adaptiveBot/internal/initialize.js", "src/edgeAuth/index.mjs"  ],
  "exclude": [
    "node_modules",
    "cdktf.out",
  ]
}
